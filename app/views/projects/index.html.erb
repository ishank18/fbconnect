<h1>All Projects</h1>
<table>
	<tr>
		<th>name</th>
		<th>user</th>
		<th>action</th>
	</tr>
	<% @projects.each do |project| %>
	<tr>
		<td><%= project.name %></td>
		<td><%= (project.users.collect { |u| u.name }).join(", ") %></td>
		<td><%= link_to "Edit", edit_project_path(project) %></td>
	</tr>
	<% end %>
</table>
